{
  "name": "peteconsuegra/laravel-manager-plugin",
  "description": "Laravel Manager plugin for WordPress Pete (create/import Laravel apps and link with WordPress).",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Pedro Consuegra", "email": "pedroconsuegrat@gmail.com" }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Pete\\LaravelManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pete\\LaravelManager\\LaravelManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
